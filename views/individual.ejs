<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8" />
    <title>BOMED Database</title>
    <link rel="stylesheet" type="text/css" href="/style.css" />
    <link
      href="https://fonts.googleapis.com/css?family=Roboto:300"
      rel="stylesheet"
    />
  </head>

  <body>
    <%- include('header') %>
    <div class="row">
      <%- include('sidebar') %>
      <div class="list-container">
        <div class="item-row legend">
          <div class="text-box noview"></div>
          <div class="text-box name">Name</div>
          <div class="text-box">Catalog #</div>
          <div class="text-box">Serial #</div>
          <div class="text-box">Location</div>
          <div class="text-box">Delivery Date</div>
          <div class="text-box">Pickup Date</div>
          <div class="text-box note">Notes</div>
        </div>
        <div class="item-row">
          <div class="text-box noview"></div>
          <div class="text-box name">
            <%= item.name %>
          </div>
          <div class="text-box">
            <%= item.catalog %>
          </div>
          <div class="text-box">
            <%= item.serial %>
          </div>
          <div class="text-box">
            <%= item.location %>
          </div>
          <div class="text-box">
            <%= item.dateDeliver %>
          </div>
          <div class="text-box">
            <%= item.datePickup %>
          </div>
          <div class="text-box note">
            <%= item.note %>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
